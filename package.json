{
  "name": "scoututor",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitz-guard/core": "0.4.1",
    "@chakra-ui/icons": "1.0.16",
    "@chakra-ui/react": "1.1.2",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@fontsource/inter": "4.5.0",
    "@prisma/client": "3.6.0",
    "blitz": "0.44.0",
    "final-form": "4.20.6",
    "framer-motion": "3.2.0",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.7",
    "react-icons": "4.3.1",
    "zod": "3.11.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.7",
    "@types/faker": "5.5.9",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.32",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "faker": "5.5.3",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.5",
    "prisma": "3.6.0",
    "typescript": "4.3.5"
  },
  "private": true
}
